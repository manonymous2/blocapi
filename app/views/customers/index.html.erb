<h1>Listing customers</h1>

<table>
  <tr>
    <th>App name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @customers.each do |customer| %>
  <tr>
    <td><%= customer.app_name %></td>
    <td><%= link_to 'Show', customer %></td>
    <td><%= link_to 'Edit', edit_customer_path(customer) %></td>
    <td><%= link_to 'Destroy', customer, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<script src="Chart.js"></script>

<%= link_to 'New Customer', new_customer_path, id: 589 %>

<script>
 function clickEvent(){
        $.ajax({
        type: "POST",
        url: "/events",
        data: {"event": {"event_name": "new_customer_path", "user_id": "<%= current_user.id rescue 0 %> "} }
      })
    }

    $("#589").click(function(){clickEvent()});
    </script>